                                    <output expression="M&#233;dia" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Infelizmente o aluno &quot; &amp;name&amp; &quot; est&#225; reprovado, devido sua m&#233;dia geral inferior a 5. Sua m&#233;dia final &#233; de:&quot;" newline="True"/>
                                    <output expression="M&#233;dia" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;O aluno &quot; &amp;name&amp; &quot;est&#225; aprovado com uma m&#233;dia geral de:&quot;" newline="True"/>
                            <output expression="M&#233;dia" newline="True"/>
                        </else>
                    </if>
                </else>
