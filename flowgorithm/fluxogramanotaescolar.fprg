        <body>
            <declare name="name" type="String" array="False" size=""/>
            <declare name="n1, n2, n3" type="Integer" array="False" size=""/>
            <declare name="M&#233;dia" type="Real" array="False" size=""/>
            <output expression="&quot;Ol&#225;, Professor! Seja bem-vindo ao nosso sistema escolar de aprova&#231;&#227;o.&quot;" newline="True"/>
            <output expression="&quot;Digite o nome do aluno para come&#231;armos: &quot;" newline="True"/>
