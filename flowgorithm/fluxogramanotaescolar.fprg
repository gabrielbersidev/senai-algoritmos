            <input variable="n2"/>
            <output expression="&quot;Por &#250;ltimo, sua terceira nota: &quot;" newline="True"/>
            <input variable="n3"/>
            <assign variable="M&#233;dia" expression="(n1+n2+n3)/3"/>
            <if expression="M&#233;dia &gt;= 7">
                <then>
                    <output expression="&quot;O aluno &quot; &amp;name&amp; &quot; est&#225; aprovado com uma m&#233;dia geral de:&quot;" newline="True"/>
