    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="teamname" type="String" array="False" size=""/>
            <declare name="tieuser" type="Integer" array="False" size=""/>
            <declare name="victoryuser" type="Integer" array="False" size=""/>
            <declare name="tiepoint" type="Integer" array="False" size=""/>
            <declare name="victorypoint" type="Integer" array="False" size=""/>
            <declare name="totalpontuacao" type="Integer" array="False" size=""/>
            <assign variable="tiepoint" expression="1"/>
            <assign variable="victorypoint" expression="3"/>
            <output expression="&quot;Seja bem-vindo ao calculador de pontos da tabela do Brasileir&#227;o!&quot;" newline="True"/>
            <output expression="&quot;Qual o nome do seu time?&quot;" newline="True"/>
