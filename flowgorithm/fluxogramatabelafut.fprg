            <input variable="victoryuser"/>
            <output expression="&quot;Quantos empates tem seu time?&quot;" newline="True"/>
            <input variable="tieuser"/>
            <assign variable="totalpontuacao" expression="(victorypoint*victoryuser)+(tieuser*tiepoint)"/>
            <output expression="&quot;Boa! Seu time tem &quot; &amp;totalpontuacao&amp; &quot;pts.&quot;" newline="True"/>
